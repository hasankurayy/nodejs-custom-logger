{
  "name": "custom-logger-beauty",
  "version": "1.0.1",
  "description": "Beautiful console logger with file context for Node.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm src/index.ts",
    "start": "npm run build && node dist/index.js",
    "example": "node --loader ts-node/esm src/examples/basic.ts",
    "example:js": "npm run build && node dist/examples/basic.js",
    "publish": "npm publish"
  },
  "keywords": [
    "logger",
    "console",
    "context",
    "express",
    "chalk",
    "debugging",
    "development",
    "log"
  ],
  "author": "Hasan Kuray <hasankryy@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasankurayy/nodejs-custom-logger.git"
  },
  "bugs": {
    "url": "https://github.com/hasankurayy/nodejs-custom-logger/issues"
  },
  "homepage": "https://github.com/hasankurayy/nodejs-custom-logger#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
